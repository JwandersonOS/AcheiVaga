<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.integrador.projintegrador">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".PrincipalActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Cadastrar_EstacionamentoActivity"
            android:parentActivityName=".SecundariaActivity" />
        <activity
            android:name=".SecundariaActivity"
            android:parentActivityName=".PrincipalActivity" />
        <activity
            android:name=".Cadastrar_UsuarioActivity"
            android:parentActivityName=".SecundariaActivity" />
        <activity
            android:name=".Lista_Estacionamento"
            android:parentActivityName=".SecundariaActivity" />
        <activity android:name=".Detalhes_Itens" />
        <activity
            android:name=".Activity_Principal"
            android:label="@string/title_activity__principal"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>